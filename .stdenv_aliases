# ex: set filetype=sh: #

[ ${sudomr_hooks+x} ] && sudomr_hooks=()
sudomrenv() {
	case "$HOSTNAME" in \
		stdenv) echo "dummy message";;
	esac
}
sudomr_hooks+=('sudomrenv')
stdenvdir="$STDHOME_DIRNAME/../stdenv"
alias gitenv="git --git-dir=$stdenvdir/.git --work-tree=~"
alias gitenvworkon="export GIT_DIR=$stdenvdir/.git; export GIT_WORK_TREE=~"
alias cdstdenv="cd $stdenvdir"

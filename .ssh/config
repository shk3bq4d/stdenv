Host *
	Ciphers aes256-gcm@openssh.com,aes128-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
	KexAlgorithms curve25519-sha256,curve25519-sha256@libssh.org,diffie-hellman-group14-sha256,diffie-hellman-group16-sha512,diffie-hellman-group18-sha512,ecdh-sha2-nistp521,ecdh-sha2-nistp384,ecdh-sha2-nistp256,diffie-hellman-group-exchange-sha256
	MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha2-256

Include config-std*

Match exec "test  $(uname -o)  =  Cygwin"
    ControlMaster no
    ControlPath none
    ControlPersist no
Match exec "test  $(uname -o)  =  Msys"
    ControlMaster no
    ControlPath none
    ControlPersist no

Host *tunnels *tunnel tunnel-*
    IdentitiesOnly yes
    ControlMaster no
    ControlPath none
    ControlPersist no
    ExitOnForwardFailure yes

Host *
    ControlMaster auto
    #ControlPath ~/.ssh/c/%C
    ControlPersist 45m
Host *-tunnel tunnel-*
    ControlPath ~/.ssh/c/tunnel-%h_%p_%r
Host jump-prod.greypay.net jump.greypay.net jump-staging.greypay.net
    ControlPath ~/.ssh/c/%C
Host *
    ControlPath ~/.ssh/c/%h_%p_%r
Host *
    ServerAliveInterval 30
Host *
    ServerAliveCountMax 5

